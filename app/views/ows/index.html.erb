<p id="notice"><%= notice %></p>

<h1>Ows</h1>

<table>
  <thead>
    <tr>
      <th>A1</th>
      <th>A2</th>
      <th>A3</th>
      <th>A4</th>
      <th>A5</th>
      <th>A6</th>
      <th>A7</th>
      <th>A8</th>
      <th>A9</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ows.each do |ow| %>
      <tr>
        <td><%= ow.a1 %></td>
        <td><%= ow.a2 %></td>
        <td><%= ow.a3 %></td>
        <td><%= ow.a4 %></td>
        <td><%= ow.a5 %></td>
        <td><%= ow.a6 %></td>
        <td><%= ow.a7 %></td>
        <td><%= ow.a8 %></td>
        <td><%= ow.a9 %></td>
        <td><%= link_to 'Show', ow %></td>
        <td><%= link_to 'Edit', edit_ow_path(ow) %></td>
        <td><%= link_to 'Destroy', ow, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ow', new_ow_path %>
