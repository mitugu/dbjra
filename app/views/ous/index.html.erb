<p id="notice"><%= notice %></p>

<h1>Ous</h1>

<table>
  <thead>
    <tr>
      <th>A1</th>
      <th>A2</th>
      <th>A3</th>
      <th>A4</th>
      <th>A5</th>
      <th>A6</th>
      <th>A7</th>
      <th>A8</th>
      <th>A9</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ous.each do |ou| %>
      <tr>
        <td><%= ou.a1 %></td>
        <td><%= ou.a2 %></td>
        <td><%= ou.a3 %></td>
        <td><%= ou.a4 %></td>
        <td><%= ou.a5 %></td>
        <td><%= ou.a6 %></td>
        <td><%= ou.a7 %></td>
        <td><%= ou.a8 %></td>
        <td><%= ou.a9 %></td>
        <td><%= link_to 'Show', ou %></td>
        <td><%= link_to 'Edit', edit_ou_path(ou) %></td>
        <td><%= link_to 'Destroy', ou, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ou', new_ou_path %>
